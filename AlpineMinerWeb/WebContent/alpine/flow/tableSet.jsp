<%--
<script type="text/javascript" src="../../js/alpine/props/tableset.js"></script>

<fmt:bundle basename="app">
	<div dojoType="dijit.Dialog" id="operator_dataset_mainDialog" draggable="false" title="<fmt:message key='tableset_config_title'/>">
		<div dojoType="dijit.layout.BorderContainer" style="width: 800px; height: 400px">
			<div dojoType="dijit.layout.ContentPane" region="top">
				<table width="100%">
					<tr>
						<td>
							<label><fmt:message key="tableset_config_title_type"/></label>
							<select dojoType="dijit.form.Select" id="operator_dataset_editor_type">
								<option value="UNION" selected>UNION</option>
								<option value="UNION ALL">UNION ALL</option>
								<option value="INTERSECT">INTERSECT</option>
								<option value="EXCEPT">EXCEPT</option>
							</select>
						</td>
						<td>
							<div id="tableset_config_firstTable_area" style="display: none;">
								<label><fmt:message key="tableset_config_title_first_table"/></label>
								<select dojoType="dijit.form.Select" id="operator_dataset_editor_firstTable">
								</select>
							</div>
						</td>
					</tr>
				</table>
			</div>
			<div dojoType="dijit.layout.ContentPane" region="center">
				<div id="operator_dataset_editGrid_container" style="width: 450px; height: 90%"></div>
				<div align="left">
					<button type="Button" baseClass="workflowButton" dojoType="dijit.form.Button" id="operator_dataset_editor_button_add"><fmt:message key="tableset_config_button_add"/></button>
					<button type="Button" baseClass="workflowButton" dojoType="dijit.form.Button" id="operator_dataset_editor_button_remove"><fmt:message key="tableset_config_button_remove"/></button>
				</div>
			</div>
			<div dojoType="dijit.layout.ContentPane" region="right" style="width: 300px;">
				<table cellspacing="5">
					<tr>
						<td nowrap>
							<input type="hidden" id="operator_dataset_editor_rowID">
							<button type="Button" baseClass="workflowButton" dojoType="dijit.form.Button" id="operator_dataset_editor_button_save"><fmt:message key="tableset_config_button_save"/></button>
						</td>
					</tr>
					<tr>
						<td>
							<label class="valueLabel">
								<fmt:message key="tableset_config_grid_header_customized_name"/>
							</label>
						</td>
						<td>
							<input id="operator_dataset_editor_outputName"></input>
						</td>
					</tr>
					<tbody id="operator_dataset_editor_table"></tbody>
				</table>
			</div>
		</div>
		<div class="dialogFooter">
			<button type="Button" dojoType="dijit.form.Button" baseClass="dialogButton" id="operator_dataset_button_save"><fmt:message key="tableset_config_button_save"/></button>
			<button type="Button" dojoType="dijit.form.Button" baseClass="dialogButton" id="operator_dataset_button_close"><fmt:message key="tableset_config_button_close"/></button>
		</div>
	</div>
</fmt:bundle>--%>
